{
  "api": {
    "ElectionsAPI": {
      "dependsOn": [
        {
          "attributes": ["UserPoolId"],
          "category": "auth",
          "resourceName": "stotraelections"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "API Gateway"
    }
  },
  "auth": {
    "stotraelections": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": ["SMS"],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": ["EMAIL"],
        "socialProviders": [],
        "usernameAttributes": ["EMAIL"],
        "verificationMechanisms": ["EMAIL"]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "function": {
    "ElectionsLambda": {
      "build": true,
      "dependsOn": [
        {
          "attributes": ["UserPoolId"],
          "category": "auth",
          "resourceName": "stotraelections"
        }
      ],
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  },
  "storage": {
    "ElectionsDB": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB"
    }
  }
}